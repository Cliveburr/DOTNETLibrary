<Window x:Class="RefineryBoard.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RefineryBoard"
        mc:Ignorable="d"
        Title="Refinery Board" Height="665.59" Width="930.843" WindowStartupLocation="CenterScreen" Closed="Window_Closed">
    <Grid>
        <TabControl x:Name="tabActions">
            <TabItem Header="Activity">
                <StackPanel Background="#FFE5E5E5">
                    <Button Margin="4" HorizontalAlignment="Left" Width="100" Click="AuctionActivity_Click">Auction</Button>
                    <Button Margin="4" HorizontalAlignment="Left" Width="100" Click="OLXActivity_Click">OLX</Button>
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
